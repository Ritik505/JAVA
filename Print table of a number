import java.util.*;
public class loops{
    public static void main(String args[]){
        System.out.print("Enter the number: ");
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for(int i=1;i<11;i++){
        System.out.println(n +"*"+i+"="+(n*i));
        }
    }
}
